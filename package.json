{
  "name": "webpack-typescript",
  "version": "1.0.0",
  "description": "Typescript & Webpack",
  "author": "Tom Lingham",
  "scripts": {
    "dev": "webpack-serve -r ./webpack/register-ts.js --config webpack/webpack.config.dev.ts",
    "build": "NODE_ENV=production webpack -r ../../../webpack/register-ts.js --config ./webpack/webpack.config.prod.ts",
    "type-check": "tsc --noEmit",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.51",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-react": "^7.0.0-beta.51",
    "@babel/preset-typescript": "^7.0.0-beta.51",
    "@babel/register": "^7.0.0-beta.51",
    "@types/html-webpack-plugin": "^2.30.3",
    "@types/invariant": "^2.2.29",
    "@types/jest": "^23.1.0",
    "@types/node": "^10.3.3",
    "@types/react": "^16.3.17",
    "@types/react-dom": "^16.0.6",
    "@types/react-test-renderer": "^16.0.1",
    "@types/warning": "^3.0.0",
    "@types/webpack": "^4.4.0",
    "@types/webpack-serve": "^1.0.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "babel-loader": "^8.0.0-beta.2",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-styled-components": "^1.5.1",
    "html-webpack-plugin": "^3.2.0",
    "invariant": "^2.2.4",
    "jest": "^23.1.0",
    "prettier": "~1.12.1",
    "react-hot-loader": "^4.3.2",
    "react-test-renderer": "^16.4.1",
    "typescript": "^2.9.1",
    "warning": "^4.0.1",
    "webpack": "^4.8.3",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^2.1.3",
    "webpack-serve": "^1.0.2"
  },
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "styled-components": "^3.2.6"
